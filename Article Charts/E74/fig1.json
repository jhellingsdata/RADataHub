{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "config": {
        "font": "Circular Std",
        "view": {"stroke": "transparent"},
        "axis": {
          "labelColor": "#676A86",
          "labelFontSize": 11,
          "labelFont": "Circular Std",
          "labelOpacity": 0.7,
          "tickColor": "#676A86",
          "tickOpacity": 0.5,
          "domainColor": "#676A86",
          "domainOpacity": 0.5,
          "gridColor": "#676A86",
          "gridDash": [2, 2],
          "gridOpacity": 0.5,
          "titleColor": "#676A86",
          "titleOpacity": 0.8,
          "tickSize": 4
        },
        "axisXDiscrete": {
          "grid": false,
          "labelAngle": 0,
          "tickCount": 10,
          "tickOpacity": 0.5
        },
        "scale":{
          "zero": false,
          "continuousPadding": 10
        },
        "axisYDiscrete": {"ticks": false, "labelPadding": 5},
        "axisXQuantitative": {
          "grid": false,
          "format":".0%"
        },
        "axisXTemporal": {"grid": false},
        "axisYQuantitative": {
          "gridColor": "#676A86",
          "gridDash": [1, 5],
          "gridOpacity": 0.5,
          "ticks": false,
          "labelPadding": 5,
          "tickCount": 8,
          "titleAngle": 0,
          "titleAlign": "left",
          "titleBaseline": "bottom",
          "titleX": 0,
          "titleY": -5,
          "format":".0%",
          "grid": false
        },
        "legend": {
          "titleColor": "#122b39",
          "title": null,
          "labelColor": "#122b39",
          "labelFontSize": 11,
          "orient": "top-right"
        }
    }
  }