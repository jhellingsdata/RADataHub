        "scale": {
            "zero": true
        },
        "axisYDiscrete": {
            "ticks": false,
            "labelPadding": 5
        },
        "axisYQuantitative": {
            "gridColor": "#676A86",
            "gridDash": [
                1,
                5
            ],
            "gridOpacity": 0.5,
            "ticks": false,
            "labelPadding": 5,
            "tickCount": 8,
            "titleAngle": 0,
            "titleAlign": "left",
            "titleBaseline": "bottom",
            "titleX": 0,
            "titleY": -5
