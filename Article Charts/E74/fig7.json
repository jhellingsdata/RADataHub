{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "config": {
        "font": "Circular Std",
        "view": {
            "stroke": "transparent"
        },
        "range": {
            "category": [
                "#36B7B4",
                "#E6224B",
                "#F4C245",
                "#0063AF",
                "#00A767",
                "#179FDB",
                "#EB5C2E"
            ]
        },
        "axis": {
            "labelColor": "#676A86",
            "labelFontSize": 11,
            "labelFont": "Circular Std",
            "labelOpacity": 0.7,
            "tickColor": "#676A86",
            "tickOpacity": 0.5,
            "domainColor": "#676A86",
            "domainOpacity": 0.5,
            "gridColor": "#676A86",
            "gridDash": [
                2,
                2
            ],
            "gridOpacity": 0.5,
            "titleColor": "#676A86",
            "titleOpacity": 0.8,
            "tickSize": 4
